{
  "NPO": {
    "detectors": [
      {
        "presentMatcher": {
          "target": {
            "selector": "#ccm_notification"
          },
          "type": "css"
        },
        "showingMatcher": {
          "target": {
            "selector": "#ccm_notification"
          },
          "type": "css"
        }
      }
    ],
    "methods": [
      {
        "action": {
          "target": {
            "selector": "#ccm_close"
          },
          "type": "click"
        },
        "name": "HIDE_CMP"
      }
    ]
  },
  "NPOBANNER": {
    "detectors": [
      {
        "presentMatcher": {
          "target": {
            "selector": ".click-away"
          },
          "type": "css"
        },
        "showingMatcher": {
          "target": {
            "selector": ".click-away"
          },
          "type": "css"
        }
      }
    ],
    "methods": [
      {
        "action": {
          "parent": {
            "selector": ".click-away"
          },
          "target": {
            "selector": ".close"
          },
          "type": "click"
        },
        "name": "HIDE_CMP"
      }
    ]
  },
  "Reddit": {
    "detectors": [
      {
        "presentMatcher": {
          "target": {
            "selector": "._3q-XSJ2vokDQrvdG6mR__k"
          },
          "type": "css"
        },
        "showingMatcher": {
          "target": {
            "selector": "._3q-XSJ2vokDQrvdG6mR__k"
          },
          "type": "css"
        }
      }
    ],
    "methods": [
      {
        "name": "HIDE_CMP",
        "action": {
          "parent": {
            "selector": "._3q-XSJ2vokDQrvdG6mR__k"
          },
          "target": {
            "selector": "button",
            "textFilter": "I Agree"
          },
          "type": "click"
        }
      }
    ]
  },
  "DPG": {
    "detectors": [
      {
        "presentMatcher": {
          "target": {
            "selector": "[id^=sp_message_iframe]"
          },
          "type": "css"
        },
        "showingMatcher": {
          "target": {
            "selector": "[id^=sp_message_iframe]"
          },
          "type": "css"
        }
      }
    ],
    "methods": [
      {
        "action": {
          "parent": {
            "iframeFilter": true,
            "selector": ".message-container"
          },
          "target": {
            "selector": "[class='message_component'] button",
            "textFilter": ["Instellen", "Opties"]
          },
          "type": "click"
        },
        "name": "OPEN_OPTIONS"
      }
    ]
  }
}
