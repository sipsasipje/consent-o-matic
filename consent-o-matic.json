{
  "NPO": {
    "detectors": [
      {
        "presentMatcher": {
          "target": {
            "selector": "#ccm_notification"
          },
          "type": "css"
        },
        "showingMatcher": {
          "target": {
            "selector": "#ccm_notification"
          },
          "type": "css"
        }
      }
    ],
    "methods": [
      {
        "action": {
          "actions": [
            {
              "target": {
                "selector": "#ccm_close"
              },
              "type": "click"
            },
            {
              "type": "wait",
              "waitTime": 250
            },
            {
              "parent": {
                "selector": ".click-away"
              },
              "target": {
                "selector": ".close"
              },
              "type": "click"
            }
          ],
          "type": "list"
        },
        "name": "HIDE_CMP"
      }
    ]
  },
  "Reddit": {
    "detectors": [
      {
        "presentMatcher": {
          "target": {
            "selector": "._3q-XSJ2vokDQrvdG6mR__k"
          },
          "type": "css"
        },
        "showingMatcher": {
          "target": {
            "selector": "._3q-XSJ2vokDQrvdG6mR__k"
          },
          "type": "css"
        }
      }
    ],
    "methods": [
      {
        "name": "HIDE_CMP",
        "action": {
          "parent": {
            "selector": "._3q-XSJ2vokDQrvdG6mR__k"
          },
          "target": {
            "selector": "button",
            "textFilter": "I Agree"
          },
          "type": "click"
        }
      }
    ]
  }
}
